{
  "name": "@gad/templates",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "default": "./dist/render.js"
    },
    "./types": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    }
  },
  "dependencies": {
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "typescript": "^5.7.2"
  }
}
